{
  "manifest_version": 3,
  "name": "adBlockube",
  "version": "1.0",
  "description": "Saltar anuncios de YouTube.",
  "permissions": ["tabs", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  }
}